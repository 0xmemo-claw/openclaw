{
  "manifest_version": 3,
  "name": "MetaMask Dummy Extension",
  "version": "1.0.0",
  "description": "Dummy MetaMask extension for testing OpenClaw browser integration",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
